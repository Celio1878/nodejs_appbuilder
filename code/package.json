		"cors": "^2.8.5",
		"express": "^4.17.3",
		"pino": "^7.9.2",
		"pino-pretty": "^7.5.4",
		"slugify": "^1.6.5"
	},
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/supertest": "2.0.10",
